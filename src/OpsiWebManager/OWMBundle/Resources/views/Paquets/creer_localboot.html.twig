{% extends "OWMBundle::layout.html.twig" %}
{% block body %}

<form action="{{ path('Paquets_creer') }}" method="post" {{ form_enctype(form) }}>


<div class="row" align="center">
          <div class="span12" align="left">
            <h2>Information</h2>
		<p>{{ form_label(form.type) }}
{{ form_widget(form.type) }}</p>


<p>{{ form_label(form.productId) }}
{{ form_widget(form.productId) }}</p>


<p>{{ form_label(form.productName) }}
{{ form_widget(form.productName) }}</p>


<p>{{ form_label(form.description) }}
{{ form_widget(form.description) }}</p>


<p>{{ form_label(form.advice) }}
{{ form_widget(form.advice) }}</p>


<p>{{ form_label(form.productVersion) }}
{{ form_widget(form.productVersion) }}</p>


<p>{{ form_label(form.packageVersion) }}
{{ form_widget(form.packageVersion) }}</p>


<p>{{ form_label(form.licence) }}
{{ form_widget(form.licence) }}</p>


<p>{{ form_label(form.priority) }}
{{ form_widget(form.priority) }}</p>
          </div>

          <div class="span12" align="left">
            <h3>Fichiers</h3>
<p>
{{ form_widget(form.install) }}
Install
</p>
<p>
{{ form_widget(form.uninstall) }}
Uninstall
</p>
<p>
{{ form_widget(form.update) }}
Update
</p>
<p>
{{ form_widget(form.always) }}
Always
</p>
<p>
{{ form_widget(form.once) }}
Once
</p>
<p>
{{ form_widget(form.custom) }}
Custom
</p>
<p>
{{ form_widget(form.logon) }}
Logon
</p>
          </div>
        </div>
{{ form_rest(form) }}
    <input type="submit" />
</form>
{% endblock %}
